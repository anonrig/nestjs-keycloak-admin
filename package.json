{
  "name": "nestjs-keycloak-admin",
  "version": "1.1.3",
  "description": "Keycloak Admin Provider for Nest.js",
  "author": "Yagiz Nizipli <yagiz@relevantfruit.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/main/index.d.ts",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "clean": "rimraf dist",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "prepare": "npm run snyk-protect && npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "snyk-protect": "snyk protect"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/relevantfruit/nestjs-keycloak-admin"
  },
  "bugs": "https://github.com/relevantfruit/nestjs-keycloak-admin",
  "peerDependencies": {
    "@nestjs/common": "^7.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "keycloak-admin": "^1.13.0",
    "openid-client": "^3.15.3"
  },
  "devDependencies": {
    "@nestjs/common": "^7.1.3",
    "@types/axios": "^0.14.0",
    "@types/node": "14.0.13",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "snyk": "^1.342.2",
    "ts-node": "8.10.2",
    "tsc-watch": "4.2.8",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.5"
  },
  "snyk": true
}
