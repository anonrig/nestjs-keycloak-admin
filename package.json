{
  "name": "nestjs-keycloak-admin",
  "version": "1.3.1",
  "description": "Keycloak Admin Provider for Nest.js",
  "author": "Yagiz Nizipli <yagiz@relevantfruit.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/main/index.d.ts",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "style": "run-s style:*",
    "style:format": "prettier --write \"src/**/*.ts\"",
    "style:lint": "eslint src --ext .js,.ts --fix",
    "start:dev": "tsc -w",
    "clean": "rimraf dist",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "prepare": "npm run snyk-protect && npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "snyk-protect": "snyk protect",
    "release": "semantic-release"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/relevantfruit/nestjs-keycloak-admin"
  },
  "bugs": "https://github.com/relevantfruit/nestjs-keycloak-admin",
  "peerDependencies": {
    "@nestjs/common": "^7.4.2",
    "@nestjs/core": "^7.4.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.2"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "keycloak-admin": "^1.13.0",
    "keycloak-connect": "^11.0.1",
    "openid-client": "^3.15.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@nestjs/common": "^7.3.2",
    "@nestjs/core": "^7.3.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/express": "^4.17.7",
    "@types/node": "14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "commitizen": "^4.1.5",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "semantic-release": "^17.1.1",
    "snyk": "^1.381.2",
    "ts-node": "9.0.0",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.2"
  },
  "snyk": true
}
